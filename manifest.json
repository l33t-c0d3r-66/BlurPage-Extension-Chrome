{
  "manifest_version": 3,
  "name": "Blur Tab Content Extension",
  "version": "1.0",
  "description": "Extension to blur the active tab content",
  "permissions": ["storage", "declarativeContent", "activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
